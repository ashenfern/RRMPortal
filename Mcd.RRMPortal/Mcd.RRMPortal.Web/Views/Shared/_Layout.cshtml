        function launchApplication(l_url, l_windowName) {
            ResetTimer();
            var l_width = screen.availWidth / 2;
            var l_height = screen.availHeight / 2;
            var winRef;
            var l_params = 'status=1' + ',location=1' + ',status=1' + ',resizable=1' + ',scrollbars=1' + ',width=' + l_width + ',height=' + l_height + ',left=20' + ',top=20';
            var erNumber;
            try {
                if (g_urlarray.has(l_url)) {
                    winRef = g_urlarray[l_url];
                }
            } catch (err) {
                var erDescription = err.description;
                erNumber = err.number;
                Mcd.RSMSP.WebApp.WebServices.LoggingWebService.JsErrorLogging('launchApplication', erDescription, erNumber);
            }
            try {
                if (!window.popups)
                    window.popups = {};
                if (window.popups[l_windowName])
                    window.popups[l_windowName].close();
                window.popups[l_windowName] = window.open(l_url, l_windowName, l_params);
                winRef = window.popups[l_windowName];
                winRef.moveTo(20, 20);
                winRef.resizeTo(l_width, l_height);

                //windows.push(winRef);
                //g_urlarray[l_url] = winRef;
                //children[children.length] = winRef;

                var index = windows.indexOf(winRef);
                if (index <= 0) {
                    windows.push(winRef);
                    g_urlarray[l_url] = winRef;
                    children[children.length] = winRef;
                }
                setOpenerWinRef(l_url, winRef);
                winRef.focus();

            } catch (err) {
                var erDescriptionSec = err.description;
                if (erDescriptionSec.indexOf('name') != -1) {
                    if (winRef == null || winRef.closed) {
                        winRef = window.open(l_url, l_windowName, l_params);
                        winRef.moveTo(20, 20);
                        winRef.resizeTo(l_width, l_height);
                        windows.push(winRef);
                        g_urlarray[l_url] = winRef;
                        children[children.length] = winRef;
                    } else {
                        winRef.focus();
                    }
                } else {
                    erNumber = err.number;
                    Mcd.RSMSP.WebApp.WebServices.LoggingWebService.JsErrorLogging('launchApplication', erDescriptionSec, erNumber);
                }
            }
        }
        function setOpenerWinRef(url, winRef) {
            if (this.opener != null) {
                try {
                    this.opener.setOpenerWinRef(url, winRef);
                }
                catch (e) {
                }
            } else {
                var index = windows.indexOf(winRef);
                if (index <= 0) {
                    windows.push(winRef);
                }
            }
        }
        function CloseBrowser(caz) {
            if (caz == 1) {
                var answer = confirm("The Dashboard page will be closed. \nDo you want continue?")
                if (answer) {
                    for (var i = 0; i < children.length; i++) {
                        children[i].close();
