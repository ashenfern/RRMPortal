        function getHour() {
            var hour = nwCdownDate.getHours();
            return hour;
        }
        function getMinutes() {
            var minute = nwCdownDate.getMinutes();
            minute = parseInt(readSessionTimeout()) + minute;
            return minute;
        }
        function getSeconds() {
            var second = nwCdownDate.getSeconds();
            return second;
        }
        var cdownYear = getYear();
        var cdownMonth = getMonth();
        var cdownDay = getDay();
        var cdownHour = getHour();
        var cdownMinute = getMinutes();
        var cdownSecond = getSeconds();
        function showCountdown() {
            var countDownLabel = document.getElementById("cDownLabel");
            if (cdownMinute > 60) {
                cdownMinute = cdownMinute - 60;
                cdownHour++;
            }
            var dtNow = new Date();
            Countdown = new Date(cdownYear, cdownMonth - 1, cdownDay, cdownHour, cdownMinute, cdownSecond);
            var mSecondsToCoundown = Countdown.getTime() - dtNow.getTime();
            var rest = Math.floor(mSecondsToCoundown / 1000);
            var CountdownText = "";
            if (rest >= 31536000) {
                var years = Math.floor(rest / 31536000);
                rest = rest - years * 31536000;
                if (years > 1 || years == 0) {
                    CountdownText += years + " years ";
                } else if (years == 1) {
                    CountdownText += years + " years ";
                }
            }
            if (rest >= 86400) {
                var days = Math.floor(rest / 86400);
                rest = rest - days * 86400;
                if (days > 1 || days == 0) {
                    CountdownText += days + " days  ";
                } else if (days == 1) {
                    CountdownText += days + " day  ";
                }
            }
            if (rest >= 3600) {
                var hour = Math.floor(rest / 3600);
                rest = rest - hour * 3600;
                if (hour > 10) {
                    CountdownText += hour + ":";
                } else if (hour > 1 && hour < 10 || hour == 0) {
                    CountdownText += " 0" + hour + ":";
                } else if (hour == 1) {
                    CountdownText += " 0" + hour + ":";
                }
            }
            if (rest >= 60) {
                var minutes = Math.floor(rest / 60);
                rest = rest - minutes * 60;
                if (minutes > 10) {
                    CountdownText += minutes + ":";
                } else if (minutes > 1 && minutes < 10 || minutes == 0) {
                    CountdownText += "0" + minutes + ":";
                } else if (minutes == 1) {
                    CountdownText += "0" + minutes + ":";
                }
            }
            if (rest >= 10) {
                CountdownText += rest;
            } else if (rest > 1 && rest < 10 || rest == 0) {
                CountdownText += "0" + rest;
            } else if (rest == 1) {
                CountdownText += "0" + rest;
            }
            if (CountdownText == "") {
                CountdownText = "User logged out by system:" + cdownHour + ":" + cdownMinute + ":" + cdownSecond + "  " + cdownDay + "/" + cdownMonth + "/" + cdownYear;
            }
            countDownLabel.innerHTML = CountdownText;
            if (ADFSModeEnabled != '1') {
                if (CountdownText == "1 minute 1 second ") {
                    setTimeout("ShowOneMinuteWarning()", 1000);
                }
            }
            if (CountdownText == "1 second ") {
                Mcd.RSMSP.WebApp.WebServices.LogoutService.LogoutUser();
                CloseWindows();
                window.location.href = "LoginPage.aspx";
            }
            alertTimerId = window.setTimeout("showCountdown()", 1000);
        }
